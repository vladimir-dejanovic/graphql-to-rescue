type Attendee {
    id: ID!
    name: String!
}

type Speaker {
    id: ID!
    name: String!
    twitter: String
    talks: [Talk]
}

type Talk {
    id: ID!
    # this is comment for title
    title: String!
    description: String
}

type Query {
    allTalks(size: Int): [Talk]
    allSpeakers: [Speaker]
    allAttendees: [Attendee]
}

schema {
    query: Query
}